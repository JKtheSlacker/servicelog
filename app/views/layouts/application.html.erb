<!DOCTYPE html>
<html>
<head>
  <title>ServiceLog</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= yield %>
  <% flash.each do |key, value| %>
  <div class='class' id='<%= key %>'>
    <%= value %>
  </div>
  <% end %>

</body>
</html>
